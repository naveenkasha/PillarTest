#include <iostream>
#include <string>
#include <vector>


using namespace std;

class RomanNumeral
{
public:
  RomanNumeral();
  ~RomanNumeral(){};
  string GetRomanSymbol(int number);
  
private:
    vector <string> romanVec;
};

RomanNumeral::RomanNumeral()
{
  romanVec.push_back("I");
  romanVec.push_back("V");
  romanVec.push_back("X");
}


string RomanNumeral::GetRomanSymbol(int number)
{
  if (number == 1)
    return romanVec.at(0);
  else if (number == 5)
    return romanVec.at(1);
  else 
    return romanVec.at(2);
}

#include <iostream>
#include <string>


using namespace std;

class RomanNumeral
{
public:
  RomanNumeral(){};
  ~RomanNumeral(){};
  string GetRomanSymbol(int number);
};


string RomanNumer::GetRomanSymbol(int number)
{
  if (number == 1)
    return "I";
  else if (number == 5)
    return "V";
  else 
    return "X";
}
